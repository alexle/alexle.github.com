{
  "name": "alexle.github.com",
  "version": "1.0.0",
  "description": "Personal blog",
  "devDependencies": {
    "husky": "^9.1.7",
    "stylelint": "^16.25.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-scss": "^6.12.1",
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "prepare": "husky",
    "lint": "stylelint \"**/*.scss\" --ignore-path .gitignore",
    "lint:fix": "stylelint \"**/*.scss\" --ignore-path .gitignore --fix",
    "jekyll:serve": "bundle exec jekyll serve --drafts",
    "jekyll:build": "bundle exec jekyll build",
    "dev": "concurrently \"npm run jekyll:serve\" \"npm run lint -- --watch\"",
    "build": "npm run lint && npm run jekyll:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexle/alexle.github.com.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/alexle/alexle.github.com/issues"
  },
  "homepage": "https://github.com/alexle/alexle.github.com#readme"
}
